{% extends 'base.html.twig' %}

{% block title %}EnvironmentStatuses index{% endblock %}

{% block body %}
    <h1>EnvironmentStatuses index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for environment_status in environment_statuses %}
            <tr>
                <td>{{ environment_status.id }}</td>
                <td>{{ environment_status.status }}</td>
                <td>
                    <a href="{{ path('app_environment_statuses_show', {'id': environment_status.id}) }}">show</a>
                    <a href="{{ path('app_environment_statuses_edit', {'id': environment_status.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_environment_statuses_new') }}">Create new</a>
{% endblock %}
