{% extends 'base.html.twig' %}

{% block title %}OperatingSystems{% endblock %}

{% block body %}
    <h1>OperatingSystems</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ operating_system.id }}</td>
            </tr>
            <tr>
                <th>Breed</th>
                <td>{{ operating_system.breed }}</td>
            </tr>
            <tr>
                <th>Release</th>
                <td>{{ operating_system.release }}</td>
            </tr>
            <tr>
                <th>Alias</th>
                <td>{{ operating_system.alias }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ operating_system.description }}</td>
            </tr>
            <tr>
                <th>Sessions</th>
                <td>{{ sessions }}</td>
            </tr>
            <tr>
                <th>Supported</th>
                <td>{{ operating_system.supported ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_operating_systems_index') }}">back to list</a>

    <a href="{{ path('app_operating_systems_edit', {'id': operating_system.id}) }}">edit</a>

    {{ include('operating_systems/_delete_form.html.twig') }}
{% endblock %}
