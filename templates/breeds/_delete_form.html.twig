<form method="post" action="{{ path('app_breeds_delete', {'id': breed.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ breed.id) }}">
    <button class="btn">Delete</button>
</form>
